{
  "name": "Infinite-Opportunities",
  "version": "0.0.1",
  "description": "Trading monitor",
  "productName": "Opportunities",
  "author": "Benjamin Tourrette <benjamintourrettepro@gmail.com>",
  "private": true,
  "scripts": {
    "linkShared": "yarn link \"trading-shared\"",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "clearViteCache": "rm -rf node_modules/vite/node_modules",
    "dev": "yarn clearViteCache && quasar dev",
    "prebuild": "yarn install",
    "build": "quasar build",
    "cy:open": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://127.0.0.1:8080 \"cypress open --e2e\"",
    "cy:run": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://127.0.0.1:8080 \"cypress run --e2e\"",
    "test:component": "cross-env NODE_ENV=test cypress open --component",
    "test:component:ci": "cross-env NODE_ENV=test cypress run --component"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.1",
    "@onesignal/onesignal-vue3": "^2.0.1",
    "@quasar/app": "^3.3.3",
    "@quasar/cli": "^2.3.0",
    "@quasar/extras": "^1.16.4",
    "@stripe/stripe-js": "^2.2.0",
    "@supabase/supabase-js": "^2.39.0",
    "axios": "^1.5.0",
    "cordova-find-config": "^0.1.2",
    "lightweight-charts": "^4.1.1",
    "node-sass": "^9.0.0",
    "onesignal": "^0.1.2",
    "onesignal-sdk": "^0.0.1",
    "onesignal-vue": "^2.0.1",
    "pinia": "^2.0.11",
    "pinia-plugin-persistedstate": "^3.2.0",
    "quasar": "^2.6.0",
    "sass": "1.33.0",
    "sass-loader": "^13.3.2",
    "trading-shared": "../../Shared",
    "uuid": "^9.0.1",
    "vue": "^3.0.0",
    "vue-i18n": "^9.4.1",
    "vue-router": "^4.0.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@quasar/app-vite": "^1.3.0",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^5.1.1",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "cypress": "^13.6.1",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-cypress": "^2.14.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.17.0",
    "prettier": "^2.5.1",
    "start-server-and-test": "^2.0.3",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19 || ^19 || ^20",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
