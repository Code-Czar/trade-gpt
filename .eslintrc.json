{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard-with-typescript",
        "plugin:vue/vue3-essential", 
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "vue", 
        "prettier"
    ],
    "rules": {
        "prettier/prettier": ["error"]
    },
    "parser": "@typescript-eslint/parser"
}


